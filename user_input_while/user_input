def string_input():
    message = input("Tell me something, and I will repeat it back to you: ")
    print(message)

def int_input():
    age_str = input("enter ur age:")
    age = int(age_str)
    if age < 2:
        person = 'a baby'
    elif age >=2 and age < 4:
        person = 'a todler'
    elif age>=4 and age < 13:
        person = 'a kid'
    elif age >=13 and age < 20:
        person = 'a teenager'
    elif age>=20 and age < 65:
        person = 'an adult'
    else :
        person = 'an elder'
    print("You're "+person)

        

string_input()
int_input()
